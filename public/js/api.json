                    "application\/json": {
                        "schema": {
                            "type": "object",
                            "required": [
                                "message"
                            ],
                            "properties": {
                                "message": {
                                    "description": "Authorization Exception",
                                    "type": "string",
                                    "nullable": false,
                                    "example": "Subject <Subject Id> is not authorized for <Resource Type> <Resource Id>"
                                }
                            }
                        }
                    }
                }
            },

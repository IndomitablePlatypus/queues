                    "application\/json": {
                        "schema": {
                            "type": "object",
                            "required": [
                                "message",
                                "errors"
                            ],
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "nullable": false,
                                    "example": "The given data was invalid."
                                },
                                "errors": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    "nullable": false,
                                    "example": {
                                        "field": [

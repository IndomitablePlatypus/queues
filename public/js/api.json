        "responses": {
            "ParametersAssertionException": {
                "description": "Bad Request",
                "content": {
                    "application\/json": {
                        "schema": {
                            "type": "object",
                            "required": [
                                "message"
                            ],
                            "properties": {
                                "message": {
                                    "description": "Parameters Assertion Exception",
                                    "type": "string",
                                    "nullable": false,
